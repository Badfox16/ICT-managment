<?php
class SoftwareDTO
{

    private $Id_Software;
    private $Id_Computador;
    private $NomeSoftware;
    private $Fabricante;
    private $Versao;
    private $Estado;
    private $Observacoes;
    private $Categoria;
    private $DiaInstalacao;
    private $PrazoSoftware;
    private $DiaExpiracao;

    // Getter methods
    public function getIdSoftware()
    {
        return $this->Id_Software;
    }

    public function getIdComputador()
    {
        return $this->Id_Computador;
    }

    public function getNomeSoftware()
    {
        return $this->NomeSoftware;
    }

    public function getFabricante()
    {
        return $this->Fabricante;
    }

    public function getVersao()
    {
        return $this->Versao;
    }

    public function getEstado()
    {
        return $this->Estado;
    }

    public function getObservacoes()
    {
        return $this->Observacoes;
    }

    public function getCategoria()
    {
        return $this->Categoria;
    }

    public function getDiaInstalacao()
    {
        return $this->DiaInstalacao;
    }

    public function getPrazoSoftware()
    {
        return $this->PrazoSoftware;
    }

    public function getDiaExpiracao()
    {
        return $this->DiaExpiracao;
    }

    // Setter methods
    public function setIdSoftware($idSoftware)
    {
        $this->Id_Software = $idSoftware;
    }

    public function setIdComputador($idComputador)
    {
        $this->Id_Computador = $idComputador;
    }

    public function setNomeSoftware($nomeSoftware)
    {
        $this->NomeSoftware = $nomeSoftware;
    }

    public function setFabricante($fabricante)
    {
        $this->Fabricante = $fabricante;
    }

    public function setVersao($versao)
    {
        $this->Versao = $versao;
    }

    public function setEstado($estado)
    {
        $this->Estado = $estado;
    }

    public function setObservacoes($observacoes)
    {
        $this->Observacoes = $observacoes;
    }

    public function setCategoria($categoria)
    {
        $this->Categoria = $categoria;
    }

    public function setDiaInstalacao($diaInstalacao)
    {
        $this->DiaInstalacao = $diaInstalacao;
    }

    public function setPrazoSoftware($prazoSoftware)
    {
        $this->PrazoSoftware = $prazoSoftware;
    }

    public function setDiaExpiracao($diaExpiracao)
    {
        $this->DiaExpiracao = $diaExpiracao;
    }
}
